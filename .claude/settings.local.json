{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "WebFetch(domain:github.com)",
      "WebFetch(domain:pkg.go.dev)",
      "Bash(go mod init:*)",
      "Bash(mkdir:*)",
      "Bash(go get:*)",
      "Bash(find:*)",
      "Bash(ls:*)",
      "Bash(go mod tidy:*)",
      "Bash(go test:*)",
      "Bash(go run:*)",
      "Bash(mkdir:*)",
      "Bash(go test:*)",
      "Bash(rm:*)",
      "Bash(chmod:*)",
      "Bash(gofmt:*)",
      "Bash(touch:*)",
      "Bash(go install:*)",
      "Bash(export PATH=$PATH:$HOME/go/bin)",
      "Bash(golangci-lint run:*)",
      "Bash(goimports:*)",
      "Bash(od:*)"
    ],
    "deny": []
  }
}